{
  "manifest_version": 3,
  "name": "Amazon URL Extension",
  "version": "1.0",
  "description": "An extension to get the Amazon website URL.",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icons/i_16.ico",
    "48": "icons/i_48.ico",
    "128": "icons/i_128.ico"
  }
}
